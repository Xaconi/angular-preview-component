{
  "name": "angularpreview",
  "displayName": "angularPreview",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension/src-ext/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "angularpreview.setAngularPreview",
        "title": "Angular Preview",
        "category": "Angular Preview"
      }
    ],
    "customEditors": [
      {
        "viewType": "angularpreview.componentPreview",
        "displayName": "Angular Component Preview",
        "selector": [
          {
            "filenamePattern": "*.component.ts"
          },
          {
            "filenamePattern": "*.component.html"
          },
          {
            "filenamePattern": "*.component.scss"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "build": "ng build --configuration production --output-hashing none && tsc -p tsconfig.extension.json",
    "compile": "ng build --configuration production --output-hashing none && tsc -p tsconfig.extension.json",
    "watch": "ng build --configuration production --output-hashing none && tsc -watch -p tsconfig.extension.json",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "@angular/animations": "^15.2.0-rc.0",
    "@angular/common": "^15.2.0-rc.0",
    "@angular/compiler": "^15.2.0-rc.0",
    "@angular/core": "^15.2.0-rc.0",
    "@angular/forms": "^15.2.0-rc.0",
    "@angular/platform-browser": "^15.2.0-rc.0",
    "@angular/platform-browser-dynamic": "^15.2.0-rc.0",
    "@angular/router": "^15.2.0-rc.0",
    "angular-in-memory-web-api": "~0.15.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "vscode-test": "^1.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.0-rc.0",
    "@angular/cli": "^15.2.0-rc.0",
    "@angular/compiler-cli": "^15.2.0-rc.0",
    "@types/glob": "^8.0.1",
    "@types/jasmine": "~4.3.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.11.35",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vscode/test-electron": "^2.2.2",
    "copyfiles": "^2.4.1",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "jasmine-core": "~4.5.0",
    "jasmine-marbles": "~0.9.2",
    "jasmine-spec-reporter": "~7.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "karma-jasmine": "~5.1.0",
    "karma": "~6.4.0",
    "mocha": "^10.1.0",
    "protractor": "~7.0.0",
    "ts-node": "~10.9.0",
    "typescript": "~4.8.2"
  }
}
